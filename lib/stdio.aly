/**
 * Binding for C's `printf`, will return
 * the amount of characters it printed,
 * or a negative value if it failed.
 */
fn printf(format: str, _): int;

/**
 * Will print the given string, and then
 * append a new line. It will return the
 * amount of characters it printed, or a negative
 * value if it failed.
 */
fn println(format: str, _): int {
	mut va_list arg;
	va_start(arg, format);
	mut int x = printf(format, arg);
	va_end(arg);
	x = x + printf("\n");
	return x;
}

/**
 * See `printf`.
 */
fn print(format: str, _): int {
	mut va_list arg;
	va_start(arg, format);
	mut int x = printf(format, arg);
	va_end(arg);
	return x;
}
