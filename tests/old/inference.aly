
// c bindings
fn printf(format: str, _): int;

// single line functions
fn add(a: int, b: int): int -> return a + b;

// pow binding
fn pow(a: f64, b: f64): f64;

fn main(): int {
	x := "type inference";
	printf("%s\n", x);

	y := add(5, 5) + pow(5, 2);
	printf("(5 + 5) + (5 * 5) = %f\n", y);

	return 0;
}