ITERATIONS: int = 1000000;

func printf(format: str, _): int;

func calculateFizzBuzz(mut val: int) {
	// else ifs not supported lol
	if val % 15 == 0 {
		printf("%d/%d: fizz buzz\n", val, ITERATIONS);
	}
	if val % 5 == 0 {
		printf("%d/%d: buzz\n", val, ITERATIONS);
	}
	if val % 3 == 0 {
		printf("%d/%d: fizz\n", val, ITERATIONS);
	}
}

func main(): int {
	mut i: int = 0;
	for {
		calculateFizzBuzz(i);
		i = i + 1;
		if i > ITERATIONS {
			break;
		}
	}
	return 0;	
}