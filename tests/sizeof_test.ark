[c] func printf(fmt: str, ...);

struct Test {
    x: int,
    y: int,
    a: int,
    b: int
}

func main(): int {
    i: int;
    C::printf("sizeof i: %d\n", sizeof(i));
    C::printf("sizeof &i: %d\n", sizeof(&i));

    a: u8;
    C::printf("sizeof a: %d\n", sizeof(a));

    test: Test;
    C::printf("sizeof test: %d\n", sizeof(test));

    x: u8;
    if sizeof(x) != 1 {
        return 1;
    }

    y: s16;
    if sizeof(y) != 2 {
        return 2;
    }

    z: s64;
    if sizeof(z) != 8 {
        return 3;
    }

    return 0;
}
