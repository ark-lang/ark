[c] func printf(fmt: str, ...) -> int; // A

/**

this will fail because we borrow x into y, then
we try to use x later however the lifetime of y 
hasn't ended so it's still being borrowed. 

func main() -> int {
    x: int = 21;
    y: &int = &x;
    C::printf("x is %d\n", x);
    return 0;
}

**/

func main() -> int {
    x: int = 21;
    {
        y: &int = &x;
    }
    C::printf("x is %d\n", x);
    return 0;
}
