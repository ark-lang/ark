struct Thing {
	a: u8,
	b: u16,
	c: u32,
	d: u64,
	e: f32,
	f: f64,	
}

func main(): int {
	mut a: u8;
	mut b: u16;
	mut c: u32;
	mut d: u64;
	mut e: f32;
	mut f: f64;
	if a != 0 {
		return 1;
	}
	if b != 0 {
		return 2;
	}
	if c != 0 {
		return 3;
	}
	if d != 0 {
		return 4;
	}
	if e != 0 {
		return 5;
	}
	if f != 0 {
		return 6;
	}

	mut thing: Thing;
	if thing.a != 0 {
		return 7;
	}
	if thing.b != 0 {
		return 8;
	}
	if thing.c != 0 {
		return 9;
	}
	if thing.d != 0 {
		return 10;
	}
	if thing.e != 0 {
		return 11;
	}
	if thing.f != 0 {
		return 12;
	}

	return 0;
}