[c] func printf(fmt: str, ...);

func main(): int {
    arrayLitTest := [100, 101, 102, 103, 104];

    if arrayLitTest[1] != 101 {
        return 1;
    }

    if arrayLitTest[2] != 102 {
        return 2;
    }

    arrayLitTest[3] = 9999;
    if arrayLitTest[3] != 9999 {
        return 3;
    }

    mut i := 0;
    for i < 5 {
        printf("array index %d contains %d\n", i, arrayLitTest[i]);
        i = i + 1;
    }

    return 0;
}
