[c] func printf(fmt: str, ...) -> int;

func main() -> int {
    valid: bool = true;

    match valid {
        true => C::printf("%s\n", "true");
        false => C::printf("%s\n", "false");
    }


    foo := "bar";

    match foo {
        "hello" => C::printf("foo matches hello");
        "world" => { }
        "bar" => { award := "baz";
            C::printf("correct! you win %s\n", award); }
        _ => C::printf("default\n");
    }

    return 0;
}
