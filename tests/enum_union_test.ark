[c] func printf(fmt: str, ...): int;

enum Tree {
    Node{left: ^Tree, right: ^Tree},
    Leaf(int)
}

func main(): int {
	x := Tree::Leaf(42);
	y := Tree::Leaf(36);
	z := Tree::Node{left: &x, right: &y};

    C::printf("Tree-tag: %d\n", x);
    C::printf("Tree-tag: %d\n", y);
    C::printf("Node-tag: %d\n", z);
    return 0;
}
