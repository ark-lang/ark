func printf(format: str, _): int;

func add(a: int, b: int): int {
    return a + b;
}

func main(): int {
    y := 5;
    printf("y is %d\n", y);

    z := 21 + 5;
    printf("z is %d\n", z);

    d := 1 + 2 + 3 + 4 + 5 + 6 + 7 + 8 + 9 + 2;
    printf("d is %d\n", d);

    b := y + z;
    printf("y + z = %d\n", b);

    terry := "Terry is the name, leet coding is the game";
    printf("%s\n", terry);

    tree := 2.0f;
    printf("%f\n", tree);

    john_carmack_age := add(5, 5);
    printf("john is %d years old\n", john_carmack_age);

    ages := [ 5, 10, 15 ];
    ages_size: int = 3;
    mut i := 0;
    printf("ages are = [");
    for i < ages_size, i = i + 1 {
        printf("%d", ages[i]);
        if i != ages_size - 1 {
            printf(", ");
        }
    }
    printf("]\n");

    return 0;
}