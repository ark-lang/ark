[c] func printf(fmt: str, ...): int;

func swap(a: ^s32, b: ^s32) 
{
   mut temp: s32 = ^a;
   ^a = ^b;
   ^b = temp;
}

func main(): int 
{
    a: s32 = 5;
    b: s32 = 6;
    C::printf("a: %d, b: %d\n", a, b);
    swap(&a, &b);
    C::printf("a: %d, b: %d\n", a, b);

    return 6 - cast(int, a); // Inference problems ugh
}