[c] func printf(fmt: str, ...): int;

func add(a: int, b: int): int -> a + b;

func test(mut a: int) -> for a < 10 {
    C::printf("this is a test %d\n", a);
    a = a + 1;
}

func whatever() -> C::printf("testing the whatever() func\n");

func main(): int {
    whatever();
    z: int = 0;
    test(z);
    return 0;
}
