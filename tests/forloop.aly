fn main(): int {
	mut int i = 0;
	for {
		i = i + 1;
		if i > 10 {
			break;
		}
		printf("infinite loop\n");
	}

	printf("finished that 1 m8 %d\n", i);	// 11

	i = 0; // reset i
	for 1 {
		i = i + 1;
		if i > 19 {
			break;
		}
		printf("looping 1!\n");
	}
	printf("finished the while loop at %d\n", i); // 20

	mut int x = 0;
	for x < 10, x + 1 {
		printf("swag the police %d\n", x);
	}
}