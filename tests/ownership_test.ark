type Test struct {
    a: int,
};

func do_stuff(mut argument: int): int {
    argument = 21;
    return argument;
}

func main(): int {
    mut local: int = 10;
    local = do_stuff(local);
    local = 20;

    mut test: Test;
    test.a = 21;
    test.a = do_stuff(test.a);
    test.a = 31;

    return 0;
}
