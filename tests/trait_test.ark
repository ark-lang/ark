[c] func printf(fmt: str, ...);

trait ToString {
    func toString(): str;
}

struct Foo {
    x: int,
}

impl Foo {
    func bar() {
        printf("%s\n", "bar");
    }
}

impl Foo for ToString {
    func toString(): str {
        return "foo";
    }
}

func main(): int {
    mut thing: Foo;
    thing.x = 42;

    printf("thing.x: %d\n", thing.x);

    return 0;
}
