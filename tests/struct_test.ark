struct EmptyStruct {}

[c] func printf(fmt: str, ...): int;

func main(): int {
    mut thing: Test = {z: 42};

    C::printf("thing.z: %d\n", thing.z);
    if thing.z != 42 {
        return 1;
    }

    thing.y = 9;
    C::printf("thing.y: %d\n", thing.y);
    if thing.y != 9 {
        return 2;
    }

    thing.test2.x = 11;
    C::printf("thing.test2.x: %d\n", thing.test2.x);
    if thing.test2.x != 11 {
        return 3;
    }

    thing.test2.x = thing.test2.x + 9;
    if thing.test2.x != 20 {
        return 4;
    }

    return 0;
}


struct Test2 {
    x: int,
}

struct Test {
    x: int = 1,
    y: int,
    z: f64,
    a: f32,
    e: str,
    test2: Test2,
}
